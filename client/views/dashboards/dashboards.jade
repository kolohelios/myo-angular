h1#dashboardTitle Dashboard
.row
  .col-xs-8.col-xs-offset-2
    form(name='dashboardForm', novalidate)
      table.table-striped.table
        thead
          tr
            th Decision
            th Date
            th Results
          tr
            td: a(ui-sref='#') food
            td 2015-05-09
            td(ng-show='user') {{user.decision}}
            td(ng-hide='user') healthy food 
          tr
            td: a(ui-sref='decisionsdetail') food
            td 2015-05-08
            td comfort food
